      "source": [
        "N = int(input())\n",
        "D = [[0]*10 for _ in range(N+1)]\n",
        "\n",
        "for x in range(10) : D[1][x] = 1\n",
        "\n",
        "for i in range (2,N+1):\n",
        "  for j in range(0, 10):\n",
        "      for k in range(0,j+1):\n",
        "        D[i][j] += D[i-1][k]\n",
        "\n",
        "        D[i][j] %= 10007\n",
        "\n",
        "ans = 0\n",
        "for y in range(10): ans += D[N][y]\n",
        "ans %= 10007\n",
        "print(ans)"
      ],
