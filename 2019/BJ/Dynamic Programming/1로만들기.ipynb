        "n = int(input())\n",
        "\n",
        "d = [0]*(n+1)\n",
        "d[1] = 0\n",
        "\n",
        "for i in range(2,n+1):\n",
        "  if d[i] == 0 :\n",
        "    d[i] = d[i-1] +1\n",
        "\n",
        "    if i%3 == 0 and d[i] > d[i//3] + 1:\n",
        "      d[i] = d[i//3] +1\n",
        "    if i%2 == 0 and d[i] > d[i//2] + 1:\n",
        "      d[i] = d[i//2] + 1\n",
        "    \n",
        "\n",
        "print(d[n])"
